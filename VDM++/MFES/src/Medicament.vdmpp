class Medicament

instance variables
  private name:Types`String;
operations
	-- Medicament constructor
	public Medicament: Types`String ==> Medicament
		Medicament(n) == (name := n; return self)
	post name = n;
	
	-- Returns the medicament's name
	pure public getName: () ==> Types`String
		getName() == (return name);

end Medicament
