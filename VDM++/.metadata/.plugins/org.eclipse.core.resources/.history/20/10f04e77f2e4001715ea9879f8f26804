\begin{vdmpp}[breaklines=true]
class Medicament

types
 public String = seq of (char);
instance variables
  public name:String;
  
operations
 public Medicament: String ==> Medicament
(*@
\label{MedicalAssociated:10}
@*)
(*@
\label{Medicament:10}
@*)
  Medicament(n) == (name := n; return self)
 pre n <> []
 post name = n;
(*@
\label{getName:13}
@*)
 
 pure public getName: () ==> String
  getName() == (return name);

end Medicament
\end{vdmpp}
