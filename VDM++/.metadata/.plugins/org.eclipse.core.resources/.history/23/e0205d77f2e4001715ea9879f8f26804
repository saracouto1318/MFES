\begin{vdmpp}[breaklines=true]
class Specialty

types
 public String = seq of (char);
instance variables
  public name: String;
  
operations
(*@
\label{Specialty:9}
@*)
 public Specialty : String ==> Specialty
  Specialty(n) == (name := n; return self)
 pre n <> []
(*@
\label{getName:12}
@*)
 post name = n;
  
 pure public getName : () ==> String
  getName() == (return name);

end Specialty
\end{vdmpp}
