class HospitalTreatment
types
	public String = seq of (char);
instance variables
  public technician: Technician;
  public name: String;
  
operations

	public HospitalTreatment: String ==> Hospital
		HospitalTreatment(n) == (name := n; return self)
	post name = n;
	
	pure public getName: () ==> String
		getName() == (return name);

end HospitalTreatment
