class Date
types

instance variables
  private year: nat;
  private month: nat;
  private day: nat;
  private hour: nat;
  private min: nat;
  
operations

	public Date: nat * nat * nat * nat * nat==> Date
	Date(y, m, d, h, min) == (year := y; month := m; day := d; hour:= h; min := min; return self)
	pre y > 0 and m > 0 and m <= 12 and d > 0 and d <= 31;
	
	pure public getYear : () ==> nat
		getYear() == (return year);
		
	pure public getMonth : () ==> nat
		getMonth() == (return month);
		
	pure public getDay : () ==> nat
		getDay() == (return day);

	pure public compareDateLess : Date ==> bool
		compareDateLess(date) == (return (date.getYear() < year and date.getMonth() < month and date.getDay() < day));
		
	pure public compareDate : Date ==> bool
		compareDate(date) == (return (date.getYear() = year and date.getMonth() = month and date.getDay() = day));
end Date