class HospitalTreatment
types
	public String = seq of (char);
instance variables
  public technician: Technician;
  public name: String;
  
operations

	public HospitalTreatment: String * String ==> Hospital
		HospitalTreatment(n, a) == (name := n; address := a; medicalAssociated := {}; return self)
	post name = n and address = a and medicalAssociated = {};
	
	pure public getName: () ==> String
		getName() == (return name);

end HospitalTreatment
