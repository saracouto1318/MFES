\begin{vdmpp}[breaklines=true]
class Patient is subclass of Person

types
 public String = seq of (char);
instance variables
  healthNumber: String;
  
operations
(*@
\label{NursingTreatment:9}
@*)
(*@
\label{Patient:9}
@*)
 public Patient: String ==> Patient
  Patient(n) == ( healthNumber := n; return self)
 pre n <> []
 post healthNumber = n;
(*@
\label{getHealthNumber:13}
@*)
(*@
\label{getName:13}
@*)
 
 pure public getHealthNumber : () ==> String
  getHealthNumber() == (return healthNumber);
  

end Patient
\end{vdmpp}
